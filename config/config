# debug mode
BOT_DEBUG=true

# self host name
BOT_HOSTNAME=

# send package to remote host
# debian packages: ${SCP_USER}@${SCP_HOST}:${SCP_PREFIX}/debain_debs/
# ubuntu packages: ${SCP_USER}@${SCP_HOST}:${SCP_PREFIX}/ubuntu_debs/
SCP_ENABLE=true
SCP_USER=youmu
SCP_KEY_FILE=/home/youmu/.ssh/keyfile
SCP_HOST=192.168.0.100
SCP_PORT=22
SCP_PREFIX=/home/youmu/path_prefix

# github api key
GITHUB_KEY=
GITHUB_SSHKEY=

# gotify config
GOTIFY_URL=
GOTIFY_TOKEN=

# build information
BUILD_ARCH=

# debian build
DEBIAN_VERSION=
DEBIAN_MIRROR=

# ubuntu build
UBUNTU_VERSION=
UBUNTU_MIRROR=

